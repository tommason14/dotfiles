"*":
  Hydrogen:
    startDir: "dirOfFile"
  core:
    disabledPackages: [
      "language-python"
      "language-gfm"
    ]
    telemetryConsent: "limited"
    themes: [
      "one-dark-ui"
      "gruvbox-plus-syntax"
    ]
    useTreeSitterParsers: false
  editor:
    fontFamily: "Fira Code,Menlo, Consolas, DejaVu Sans Mono, monospace"
    fontSize: 16
    preferredLineLength: 90
    scrollPastEnd: true
    showIndentGuide: true
    softWrap: true
    softWrapAtPreferredLineLength: true
  "exception-reporting":
    userId: "15f1fea1-ce22-4d8c-8888-3d2c5c5a45dd"
  kite:
    showWelcomeNotificationOnStartup: false
  pigments:
    markerType: "native-dot"
    sassShadeAndTintImplementation: "bourbon"
  "python-black": {}
  "tree-view":
    hideIgnoredNames: true
    hideVcsIgnoredFiles: true
  welcome:
    showOnStartup: false
